                  <script type='text/javascript'>
                    $(function(){
                      const category = 'Hosting' || '';
                    
                      if (category.length > 0) ga('set', 'contentGroup2', category);
                    
                      $('#countries-li').on('click', function(e) {
                        $('#countries-dropdown').toggle('collapse');
                        e.stopPropagation();
                      })
                    
                      $(document).on('click', function(e) {
                        if (e.target.classList.contains('form-control') || e.target.classList.contains('toggler-search-button')) {
                          e.stopPropagation();
                          return;
                        }
                    
                        $('#countries-dropdown').hide();
                        $('.navbar-collapse').removeClass('show');
                        $('.search-form .form-group').css('width', '1rem');
                        $('.search-form .form-group').css('background-color', 'transparent');
                        $('.search-form .form-group').css('border', '1px solid rgba(51, 51, 51, 0)');
                        $('input[name=q]').blur();
                    
                        if (e.target.classList.contains('toggler-search-button')) {
                          $('#dropdownSearchButton').trigger('click');
                          e.stopPropagation();
                        }
                      });
                    
                      $(document).on('click', '#dropdownSearchButton', function(e) {
                        $('.navbar-collapse').removeClass('show');
                        $('#countries-dropdown').hide();
                    
                        if ($('.search-form .form-group').css('width') !== '16px') {
                          // close search
                          $('.search-form .form-group').css('width', '16px');
                          $('.search-form .form-group').css('background-color', 'transparent');
                          $('.search-form .form-group').css('border', '1px solid rgba(51, 51, 51, 0)');
                          $('input[name=q]').blur();
                          $('.search-form').blur();
                          $('.toggler-search-button').blur();
                        } else {
                          $('.search-form .form-group').css('width', '13rem');
                          $('.search-form .form-group').css('background-color', '#23232C');
                          $('.search-form .form-group').css('border', '1px solid #626f77');
                          $('input[name=q]:visible:first').val('').focus();
                        }
                        e.stopPropagation();
                      });
                    })
                  </script>
                </ul>
              </div>
        </nav>
      </div>
    </div>
    
    
    <script type='text/javascript'>
      $(function(){
        $(window).scroll(function(){
            if ($('.container-index-search').length > 0) {
              var fromTopPx = 20; // distance to trigger
              var scrolledFromtop = jQuery(window).scrollTop();
              if(scrolledFromtop > fromTopPx){
                  jQuery('.fixed-top').addClass('scrolled');
              }else{
                  jQuery('.fixed-top').removeClass('scrolled');
              }
            } else {
              jQuery('.fixed-top').addClass('scrolled');
            }
        });
      })
    </script>
    
    
    
