var appnexus_zone_id_leaderboard = "13891744";
var appnexus_zone_id_mobileleaderboard = "13891745";
var appnexus_zone_id_sidebarbottom = "13891747";
var appnexus_zone_id_sidebartop = "13891748";
var appnexus_zone_id_inline_bottom_right = "17915017";
var appnexus_zone_id_inline_top_right = "17915019";
