<script type='text/javascript'>
  $("#indicator-modal button.target").on('click', function(e) {
    // Don't actually make the click happen
    event.preventDefault();
  
    // build the target url
    var target = $(e.target);
    var pk = target.data('pk');
    var slug = target.data('slug');
    var label = target.data('label');
    var indicator_url = api_url + "/v2/companies/" + pk + '/indicator.json?indicator=' + label;
  
    // call the endpoint with the data
    $.post(indicator_url)
  
    // append to GA events
    ga("global.send", "event", "indicator", slug + "-" + pk, label);
  
    // show a thank you
  
    $('#step1').addClass('d-none');
    $('#step2').removeClass('d-none');
  
    return false;
  });
</script>


      
