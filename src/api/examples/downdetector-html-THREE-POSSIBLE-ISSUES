    <script type='text/javascript'>
      $(document).ready(function() {
        $('.rating_wrapper').off().on('click', '.rating span', function(e){
            $(this).closest('.rating_wrapper').find('.spinner').show();
            $.post($(this).data('href'), function(data, status){
                $('.spinner').hide();
                $('.rating').html('');
                $('.rating').append('<p>Thanks!</p>')
            });
            $('.rating_wrapper').off('click'); //rate just once
            $('.rating span').tooltip('hide');
        });
      
        $('.rating span').tooltip({'placement':'bottom'})
      })
    </script>
