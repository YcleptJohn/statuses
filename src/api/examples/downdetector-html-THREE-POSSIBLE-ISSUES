              .


            </p>
          </div>
        </div>
      </div>
    </div>
    <div id='fb-root'></div>
    <link rel='preload' href='https://cdn2.downdetector.com/3813b5e8c347c10/javascript/dist/vendors/combined.js' as='script'>
    <link rel='preload' href='https://cdn2.downdetector.com/3813b5e8c347c10/javascript/dist/adscript.js' as='script'>

    <script type='text/javascript'>
      var width = document.documentElement.clientWidth;
      var settings = {
        'account_dfp' : 'downdetector.co.uk',
        'locale_facebook' : 'en_US',
      };
      
      // TODO: Add Slot Level targetting.
      let targets = {'slotTargets': {
        'company': '3',
        'categories': 'Mobile Operators',
      }};
      
      const preloadedFiles = ['dist/vendors/combined.js', 'dist/adscript.js'];
      
      preloadedFiles.forEach(function(file) {
        const scriptTag = document.createElement('script');
        scriptTag.src = 'https://cdn2.downdetector.com/3813b5e8c347c10/javascript/' + file;
        document.body.appendChild(scriptTag);
      })
      
      // TODO move this to an external file as it's being used in a number of places
      
      // lazy load any images, and render the sparklines
      const $companyBox = $('.company-index:first');
      let column_width = $companyBox.width();
      
      function lazyLoadImages() {
        $("img.lazy").lazyload({
          threshold: 200,
          effect: "fadeIn"
        });
      }
      
      function setSparkLine() {
        $('.sparkline').each(function (i, chart) {
          let max, values;
          const $chart = $(chart);
          const chartValues = $chart.data('values');
      
          values = (!chartValues || chartValues.length === 0) ? [0] : chartValues;
      
          // check for any full zero companies, if so, render a straight line
          max = values.reduce(function (a, b) {
            return Math.max(a, b);
          });
      
          if (max === 0) {
            // only add the element if it doesn't exist yet
            if (!$chart.hasClass('zero')) {
              $chart.parent().append('<div class="sparkline zero"></div>')
              $chart.remove();
            }
          } else {
            $chart.attr('width', $chart.prev().width());
            sparkline.sparkline(document.querySelector('#sparkline-' + $chart.data('id')), values);
          }
        })
      }
      
      lazyLoadImages();
      setSparkLine();
      
      // when we resize the window, re-render all sparklines and logo's
      // but only if the actual size changed
      $(window).resize(function () {
        if ($companyBox.width() === column_width) return;
      
        column_width = $companyBox.width().width();
      
        lazyLoadImages();
        setSparkLine();
      });
      
      // When clicking on "show more", load all companies, and render all
      // sparklines and logo's lazy
      $('#showmore .btn').on('click', function (e) {
        e.preventDefault();
      
        ga("global.send", "event", "index", "show-more");
        $('#showmore .loading').show();
        $('#showmore a').hide();
      
        $('#more-companies-container').load('/more-companies/ #more-index-companies', function (data) {
          // Sometimes, because of timings, we end up with double companies
          // filter those and remove the second element from the DOM
          $('.sparkline[id]').each(function () {
            var ids = $('[id="' + this.id + '"]');
            if (ids.length > 1 && ids[0] == this)
              $(ids[1]).closest('.company-index').remove();
          });
      
          lazyLoadImages();
          setSparkLine();
          $('#showmore .btn').hide(); // hide all buttons
        });
      });
    </script>
    <script type="text/javascript">// <![CDATA[
