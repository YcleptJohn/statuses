<script type='text/javascript'>
  function setGrouping(tracker) {
      const isAccessibilityPage = window.location.pathname === '/accessibility/';
      const isOtherPage = !isAccessibilityPage && 'Status'.length === 0;
      const isCompanyPage = 'Game Platforms'.length > 0;
      const groupSet = tracker ? tracker + '.set' : 'set';
  
      // Set GA Grouping depending on the type of page
      (isOtherPage) ? ga(groupSet, 'contentGroup1', 'Other') : ga(groupSet, 'contentGroup1', 'Status');
  
      // Accessibility pages are flatpages on the server. Adding this here so we don't need to change it in every 
      if (isAccessibilityPage) ga(groupSet, 'contentGroup1', 'Legal'); 
  
      // Set GA category grouping for company pages only
      if (isCompanyPage) ga(groupSet, 'contentGroup2', 'Game Platforms');
  }
  
  
  
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  
  ga('create', 'UA-40229384-1', 'auto', {'allowLinker': true});
  if (window.zdconsent && window.zdconsent.gdprApplies && window.zdconsent.optins && !window.zdconsent.optins.ga) {
      ga('set', 'anonymizeIp', true);
      ga('set', 'displayFeaturesTask', null);
  }
  
  setGrouping();
  
  ga('send', 'pageview');
  
  ga('create', 'UA-41048417-1', 'auto', {'name': 'global', 'allowLinker': true});
  if (window.zdconsent && window.zdconsent.gdprApplies && window.zdconsent.optins && !window.zdconsent.optins.ga) {
