  <script type='text/javascript'>
    $(function () {
      $('[data-toggle="popover"]').popover();
    
      $(function() {
        $("img.lazy").lazyload();
      });
    
      $(document).on('click', function(e) {
        if (e.target.id != 'baseline_info_btn') {
          $('#baseline_info_btn').popover('hide');
        }
      });
    })
    
    $('body').on('dfpLoaded', function(){
      googletag.cmd.push(function() {
        googletag.pubads().setTargeting("ddPage", "steam");
        googletag.pubads().setTargeting("ddPageCategory", "Game Platforms");
        googletag.pubads().setTargeting("ddURL", window.location.href);
        googletag.pubads().setTargeting("ddDomain", "downdetector.co.uk");
        googletag.enableServices();
      });
    })
  </script>
